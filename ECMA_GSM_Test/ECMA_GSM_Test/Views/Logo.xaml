<UserControl x:Class="SMScinterionTERMINALtests.Views.Logo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SMScinterionTERMINALtests.Views"
             mc:Ignorable="d" 
             Width="Auto" Height="Auto">
    <Grid x:Name="LayoutRoot" Background="{x:Null}">
        <TextBlock Height="Auto" Margin="15,0,16,0" FontFamily="./Fonts/Fonts.zip#Arial" VerticalAlignment="Top"><Run FontFamily="./Fonts/Fonts.zip#Arial" FontSize="11" Foreground="#FF0E135F" Text="COHERENT" /></TextBlock>
        <Rectangle Margin="0,10,0,0" Fill="#FF0E135F" StrokeThickness="0" VerticalAlignment="Top" Height="14" />
        <TextBlock Height="Auto" Margin="15,9,32,0" FontFamily="./Fonts/Fonts.zip#Arial" VerticalAlignment="Top"><Run FontFamily="./Fonts/Fonts.zip#Arial" FontSize="11" Foreground="#FFFFFFFF" Text="RESEARCH" /></TextBlock>
    </Grid>
</UserControl>
